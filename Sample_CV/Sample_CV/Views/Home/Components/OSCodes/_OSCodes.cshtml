@model IEnumerable<Sample_CV.Models.OSCode>

<section class="latest-blog-section p-3 p-lg-5">
			<div class="container">
				<h2 class="section-title font-weight-bold mb-5">نمونه کدهای اوپن سورس</h2>
				<div class="row">

					@foreach ( var item in Model)
			{
				<div class="col-md-4 mb-5">
						<div class="card blog-post-card">
							<img class="card-img-top" src="~/assets/images/OSCode/@item.Image" alt="image">
							<div class="card-body">
								<h5 class="card-title"><a class="theme-link" href=@item.Url>@item.Title</a></h5>
								<p class="card-text">@item.Description</p>
								
							</div>
						</div>
					</div>
				
			}

					
				</div>
			</div>
		</section>